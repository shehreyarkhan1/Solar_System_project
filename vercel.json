{
  "version": 2,
  "builds": [
    {
      "src": "solar_system_drf/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "50mb", "runtime": "python3.11" }
    }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "solar_system_drf/wsgi.py" }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "solar_system_drf.settings"
  }
}
